{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-closing-bracket-location": 0,
    "object-curly-spacing": 0,
    "max-len": 0,
    "arrow-body-style": 0,

    "no-empty-label": 0,
    "no-labels": 2,

    "space-after-keywords": 0,
    "space-before-keywords": 0,
    "space-return-throw-case": 0,
    "keyword-spacing": 2,
  },
  "plugins": [
    "babel",
    "react"
  ]
}
