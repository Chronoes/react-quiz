{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-closing-bracket-location": 0,
    "object-curly-spacing": 0,
    "max-len": [1, 120],
    "no-confusing-arrow": 0,
    "no-param-reassign": [2, {"props": false}],
  },
  "plugins": [
    "babel",
    "react"
  ]
}
