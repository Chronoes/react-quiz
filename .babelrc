{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    ["extensible-destructuring", {"mode": "optout", "impl": "immutable"}]
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }, {
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }]
        }]
      ]
    },
    "test": {},
    "production": {}
  }
}
